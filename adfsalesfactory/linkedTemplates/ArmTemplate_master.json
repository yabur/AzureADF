{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsalesfactory"
		},
		"AzureBlobStorageLanding_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLanding'"
		},
		"AzureBlobStorageStaging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageStaging'"
		},
		"LS_AzureDataBricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_AzureDataBricks'"
		},
		"LS_LandingStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_LandingStorage'"
		},
		"LS_StagingStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_StagingStorage'"
		},
		"LS_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_databricks'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfsalesfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageLanding_connectionString": {
						"value": "[parameters('AzureBlobStorageLanding_connectionString')]"
					},
					"AzureBlobStorageStaging_connectionString": {
						"value": "[parameters('AzureBlobStorageStaging_connectionString')]"
					},
					"LS_AzureDataBricks_accessToken": {
						"value": "[parameters('LS_AzureDataBricks_accessToken')]"
					},
					"LS_LandingStorage_connectionString": {
						"value": "[parameters('LS_LandingStorage_connectionString')]"
					},
					"LS_StagingStorage_connectionString": {
						"value": "[parameters('LS_StagingStorage_connectionString')]"
					},
					"LS_databricks_accessToken": {
						"value": "[parameters('LS_databricks_accessToken')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}